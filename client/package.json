{
  "name": "spg",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.7.0",
    "dayjs": "^1.10.7",
    "fas": "^0.3.6",
    "moment": "^2.29.1",
    "puppeteer": "^11.0.0",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.2",
    "react-bootstrap-icons": "^1.6.1",
    "react-datetime-picker": "^3.4.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.1",
    "react-scripts": "^3.4.4",
    "react-test-renderer": "^17.0.2",
    "web-vitals": "^1.1.2"
