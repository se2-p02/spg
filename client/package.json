    "test": "react-scripts test --coverage --watchAll --testResultsProcessor=jest-sonar-reporter",
    "test:cov": "react-scripts test --coverage --testResultsProcessor=jest-sonar-reporter",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
